  <style>
    body {
      font-family: Arial, sans-serif;
      background-color: #f8f8f8;
      padding: 3rem;
    }

    .section-info {
      display: flex;
      margin-bottom: 20px;
      flex-wrap: wrap;
      width: 70%;
      border: 1px solid #ccc;
      border-radius: 4px;
      padding: 10px;
      background-color: #fff;

    }

    .section-info div {
      margin-right: 10px;
      padding: 0.7rem;
    }

    .section-info div label {
      font-weight: bold;
      color: #444;
      font-size: 14px;
    }

    .section-info div input[type="text"] {
      width: 100%;
      padding: 5px;
      border: 1px solid #ccc;
      border-radius: 4px;
      box-sizing: border-box;

    }


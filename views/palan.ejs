      padding: 5px;
      border: 1px solid #ccc;
      border-radius: 4px;
      box-sizing: border-box;

    }

    button {
      padding: 0.7rem;
      border-radius: 0.4rem;
      color: #fff;
      background-color: #444;
    }

    @media (max-width: 600px) {
      .section-info {
        width: 100%;
      }
    }

    .other-information {
      display: flex;
      justify-content: space-evenly;
      align-items: center;
      flex-wrap: wrap;
      width: 70%;

      border: 1px solid #ccc;
      border-radius: 4px;
      padding: 10px;
      background-color: #fff;
    }

    .info {
      margin-right: 3rem;

    }

    .indo div {
      margin-right: 10px;
      margin-top: 1rem;
      padding: 0.8rem;
    }

    .info div label {
      font-weight: bold;
      color: #444;
      font-size: 14px;
    }

    .info div input[type="text"] {
      width: 100%;
      padding: 5px;
      border: 1px solid #ccc;
      border-radius: 4px;
      box-sizing: border-box;

    }

    .info div select {
      width: 100%;
      padding: 5px;
      border: 1px solid #ccc;
      border-radius: 4px;
      box-sizing: border-box;
    }

    .info div input[type="date"] {
      width: 100%;
      padding: 5px;
      border: 1px solid #ccc;
      border-radius: 4px;
      box-sizing: border-box;

